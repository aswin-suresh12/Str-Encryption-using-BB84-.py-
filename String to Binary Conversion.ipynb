    "def str_to_binary(string):\n",
    "    \n",
    "\n",
    "    binary_list = []\n",
    "\n",
    "\n",
    "    for char in string:\n",
    "\n",
    "        binary_list.append(bin(ord(char)).zfill(8))\n",
    "\n",
    "\n",
    "    return ''.join(binary_list)\n",
    "\n",
    "Binary_rep = str_to_binary(\"The hardest choices require the strongest will\")\n",
    "print(Binary_rep)\n"
   ]
